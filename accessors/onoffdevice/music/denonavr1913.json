{
  "name":    "Denon AVR-1913",
  "version": "0.1",
  "author":  "Brad Campbell",
  "description": "
Denon AVR-1913 Accessor
=======================

The Denon AVR-1913 is a digital receiver.
",

  "ports": [
            {
              "direction": "output",
              "name":      "Name"
            },
            {
              "direction": "io",
              "name":      "Input",
              "type":      "select",
              "options":   ["PC", "Apple TV", "Internet"]
            },
            {
              "direction": "io",
              "name":      "Audio Mode",
              "type":      "select",
              "options":   ["Multi Channel Stereo, Stereo"]
            }
  ],

  "parameters": [
                  {
                    "name": "device_url",
                    "default": "http://localhost"
                  }
  ],

  "code": {
            "language": "javascript",
            "include": ["denonavr1913.js"]
          }
}